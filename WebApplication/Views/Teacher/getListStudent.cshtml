@model IEnumerable<SeniorProject.ViewModels.Teacher.ListStudentRegister>

<table id="JsonTable" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th class="text-align-left">ลำดับ</th>
            <th class="text-align-left">ชื่อนักศึกษา</th>
            <th class="text-align-left">งาน</th>
            <th class="text-align-left">รหัสนักศึกษา</th>
            <th class="text-align-left">วันเวลาที่สมัคร</th>
            <th class="text-align-left">สถานะ</th>
            <th class="text-align-left">ตัวดำเนินการ</th>
        </tr>
    </thead>
    <tbody>
        @{
            int Rows = 1;
            foreach (var Get in Model.OrderBy(f => f.register_date))
            {
                <tr>
                    <td class="text-align-center" width="50">@Rows.</td>
                    <td>@Get.student_name</td>
                    <td>@Get.job_name</td>
                    <td>@Get.s_id</td>
                    <td>@Get.register_date</td>
                    <td></td>
                    <td class="text-align-center">
                        <button class="btn btn-info btn-xs edit" value="@Get.s_id"><i class="fa fa-file-pdf-o"></i> เอกสาร</button>
                        <button class="btn btn-warning btn-xs edit" value="@Get.s_id"><i class="fa fa-pencil"></i> ส่งอนุมัติ</button>
                        <button class="btn btn-danger  btn-xs delete" value="@Get.s_id"><i class="fa fa-eraser"></i> ไม่อนุมัติ</button>
                    </td>
                </tr>
                Rows++;
            }
         }
        </tbody>
    </table>
